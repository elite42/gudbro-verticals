# WiFi Configuration (Copy this to .env.local and fill with real values)
NEXT_PUBLIC_WIFI_ENABLED=true
NEXT_PUBLIC_WIFI_SSID=ROOTS_Guest
NEXT_PUBLIC_WIFI_PASSWORD=your_wifi_password_here
NEXT_PUBLIC_WIFI_SECURITY=WPA
NEXT_PUBLIC_WIFI_HIDDEN=false

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3014/api/coffeeshop

# Venue ID
NEXT_PUBLIC_VENUE_ID=roots-danang

# Web Push Notification (VAPID Keys)
# Generate with: npx web-push generate-vapid-keys
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key
VAPID_SUBJECT=mailto:admin@yourdomain.com
